{
  "schemaVersion": 1,
  "id": "mgs-mapping-fabric",
  "version": "${version}",
  "name": "mgs-mapping-fabric",
  "description": "Map making tool for MiniGameSphere",
  "authors": [],
  "contact": {
    "repo": "https://github.com/fuby/mgs-mapping-fabric"
  },
  "license": "GPL-3.0",
  "icon": "assets/mgs-mapping-fabric/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.mgs.mgsMappingFabric.Main"
      }
    ]
  },
  "mixins": [
    "mgs-mapping-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-language-kotlin": ">=1.10.18+kotlin.1.9.22"
  }
}
